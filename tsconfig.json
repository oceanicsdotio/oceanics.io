{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@catalog/*": [
        "app/catalog/*"
      ],
      "@about/*": [
        "app/about/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "sourceMap": true,
    "rootDir": ".",
    "removeComments": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "typings.d.ts",
    "app/**/*",
    "glsl/*",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "build/types/**/*.ts", 
    "mdx-components.tsx", 
    "palette.ts"
, "examples.ts"  ],
  "exclude": [
    "node_modules",
    ".next"
  ]
}